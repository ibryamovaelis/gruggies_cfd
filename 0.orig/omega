FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $turbulentOmega;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    culvert_outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
	
	bridge_outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
	
    structure
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    atmosphere
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}


// ************************************************************************* //